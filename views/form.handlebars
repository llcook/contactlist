<div>
	<a class="nav-button btn btn-primary" href="/" role="button">View Contacts</a>
	<h1>Add New Contact:</h1>
</div>
<form id="addContact">
	<div class="form-group">
		<!-- First Name -->
		<label for="firstName">First Name</label>
		<input type="text" class="form-control" id="firstName" name="firstName" placeholder="First Name">
	</div>

	<div class="form-group">
		<!-- Last Name -->
		<label for="lastName">Last Name</label>
		<input type="text" class="form-control" id="lastName" name="lastName" placeholder="Last Name">
	</div>

	<div class="form-group">
		<!-- Contact Type -->
		<label for="contactType">Contact Type</label>
		<select id="contactType" class="form-control" name="contactType">
			<option value="Agriculture">Agriculture</option>
			<option value="Aviation">Aviation</option>
			<option value="Climate">Climate</option>
			<option value="Courts">Courts</option>
			<option value="Economy">Economy</option>
			<option value="Education">Education</option>
			<option value="Energy">Energy</option>
			<option value="Environment">Environment</option>
			<option value="Finance">Finance</option>
			<option value="Health">Health</option>
			<option value="Healthcare">Healthcare</option>
			<option value="History">History</option>
			<option value="Insurance">Insurance</option>
			<option value="Housing">Housing</option>
			<option value="Immigration">Immigration</option>
			<option value="Labor">Labor</option>
			<option value="Military">Military</option>
			<option value="Police">Police</option>
			<option value="Pollution">Pollution</option>
			<option value="Prisons">Prisons</option>
			<option value="Property">Transportation</option>
			<option value="Space">Space</option>
			<option value="Technology">Technology</option>
			<option value="Transportation">Transportation</option>
			<option value="Water">Water</option>
			<option value="Wildfires">Wildfires</option>
		</select>
	</div>

	<div class="form-group">
		<!-- Phone Number -->
		<label for="phoneNumber">Phone Number</label>
		<input type="text" class="form-control" id="phoneNumber" name="phoneNumber" placeholder="Phone Number">
	</div>

	<div class="form-group">
		<!-- Email Address -->
		<label for="emailAddress">Email Address</label>
		<input type="text" class="form-control" id="emailAddress" name="emailAddress" placeholder="Email Address">
	</div>

	<input type="submit" value="Submit" id="submit" class="btn btn-primary">
</form>